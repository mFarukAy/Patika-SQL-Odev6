ANSWER 1: SELECT AVG(rental_rate) FROM film;

ANSWER 2: SELECT COUNT(*) FROM film
          WHERE title ~~ 'C%'; -- 92

ANSWER 3: SELECT * FROM film
          WHERE rental_rate = 0.99
          ORDER BY length DESC
          LIMIT 1; -- 184

ANSWER 4: SELECT COUNT(DISTINCT replacement_cost) FROM film
          WHERE length > 150; -- 21
